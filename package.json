{
  "name": "knub",
  "version": "8.0.0",
  "description": "A bot framework for Discord",
  "author": "Miikka Virtanen <contact@mivir.fi>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.ts -- --trace-warnings",
    "precommit": "lint-staged",
    "build": "touch dist && rm -r dist && tsc",
    "prepublishOnly": "npm run build",
    "watch": "touch dist && rm -r dist && tsc --watch",
    "format": "prettier --write \"./**/*.ts\""
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "discord.js": "^11.3.2",
    "escape-string-regexp": "^1.0.5",
    "js-yaml": "^3.9.1",
    "lodash.at": "^4.6.0",
    "lodash.difference": "^4.5.0",
    "lodash.merge": "^4.6.1",
    "minimist-string": "^1.0.2",
    "reflect-metadata": "^0.1.10",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/escape-string-regexp": "0.0.30",
    "@types/js-yaml": "^3.9.0",
    "@types/lodash.at": "^4.6.3",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.15",
    "@types/winston": "^2.3.4",
    "chai": "^4.1.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "mocha": "^5.2.0",
    "prettier": "^1.10.2",
    "ts-node": "^3.2.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.4.2"
  },
  "peerDependencies": {}
}
